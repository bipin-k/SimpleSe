{
  "name": "HomePage",
  "defaultLocale": "en_US",
  "elements": [
    {
      "name": "heading",
      "locale": [
        {
          "name": "en_US",
          "locator": "css=h1[class=heading]"
        }
      ],
      "wait": {
        "until": "visible",
        "for": 10
      }
    },
    {
      "name": "checkboxesLink",
      "locale": [
        {
          "name": "en_US",
          "locator": "xpath=//a[text()='Checkboxes']"
        }
      ],
      "wait": {
        "until": "clickable",
        "for": 10
      }
    },
    {
      "name": "sampleTxtField",
      "locale": [
        {
          "name": "en_US",
          "locator": "xpath=//a[text()='textField']"
        }
      ]
    },
    {
      "name": "amazon",
      "locale": [
        {
          "name": "en_US",
          "locator": ".//a[@class = 'nav-a  '][%s]"
        }
      ],
      "wait": {
        "until": "visible",
        "for": 10
      }
    }
  ]
}
